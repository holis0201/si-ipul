<?xml version="1.0" encoding="UTF-8"?>
<web-app version="2.5" xmlns="http://java.sun.com/xml/ns/javaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd">
    <servlet>
        <servlet-name>register</servlet-name>
        <servlet-class>boundary.formRegistration</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>login</servlet-name>
        <servlet-class>boundary.formLogin</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>editProfil</servlet-name>
        <servlet-class>boundary.formEditProfil</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>history</servlet-name>
        <servlet-class>boundary.historyPage</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>history-pemesanan-kosong</servlet-name>
        <servlet-class>boundary.historyPage_kosong</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>register</servlet-name>
        <url-pattern>/register</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>login</servlet-name>
        <url-pattern>/login</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>editProfil</servlet-name>
        <url-pattern>/edit-profil</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>history</servlet-name>
        <url-pattern>/history-pemesanan</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>history-pemesanan-kosong</servlet-name>
        <url-pattern>/history-pemesanan-kosong</url-pattern>
    </servlet-mapping>
    <session-config>
        <session-timeout>
            30
        </session-timeout>
    </session-config>
    <welcome-file-list>
        <welcome-file>login</welcome-file>
    </welcome-file-list>
</web-app>
