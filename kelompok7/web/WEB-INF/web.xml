<?xml version="1.0" encoding="UTF-8"?>
<web-app version="2.5" xmlns="http://java.sun.com/xml/ns/javaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd">
    <servlet>
        <servlet-name>register</servlet-name>
        <servlet-class>boundary.registrasi</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>fieldKosong</servlet-name>
        <servlet-class>boundary.fieldKosong</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>addLapangan</servlet-name>
        <servlet-class>boundary.addLapangan</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>editProfil</servlet-name>
        <servlet-class>boundary.editProfil</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>login</servlet-name>
        <servlet-class>boundary.login</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>homeMember</servlet-name>
        <servlet-class>boundary.homeMember</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>listLapangan</servlet-name>
        <servlet-class>boundary.listLapangan</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>addBerita</servlet-name>
        <servlet-class>boundary.addBerita</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>register</servlet-name>
        <url-pattern>/register</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>fieldKosong</servlet-name>
        <url-pattern>/error</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>login</servlet-name>
        <url-pattern>/login</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>homeMember</servlet-name>
        <url-pattern>/homeMember</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>editProfil</servlet-name>
        <url-pattern>/edit-profil</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>addLapangan</servlet-name>
        <url-pattern>/add_lapangan</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>listLapangan</servlet-name>
        <url-pattern>/list_lapangan</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>addBerita</servlet-name>
        <url-pattern>/add_berita</url-pattern>
    </servlet-mapping>
    <session-config>
        <session-timeout>
            30
        </session-timeout>
    </session-config>
    <welcome-file-list>
        <welcome-file>login</welcome-file>
    </welcome-file-list>
</web-app>
